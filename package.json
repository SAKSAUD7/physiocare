    "dotenv": "^16.5.0",
    "framer-motion": "^10.16.5",
    "json2csv": "^6.0.0-alpha.2",
    "mongodb": "^6.3.0",
    "mongoose": "^8.0.1",
    "next": "14.0.3",
    "next-auth": "^4.24.11",
    "next-auth-client": "^1.5.0",
