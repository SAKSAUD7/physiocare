{
  "name": "physiocare",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=18.17.0",
    "npm": ">=9.0.0"
  },
  "homepage": "https://saksaud7.github.io/physiocare",
  "scripts": {
    "dev": "next dev",
    "dev:fast": "set NODE_OPTIONS=--max_old_space_size=4096 && next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint --fix",
    "clean": "rimraf .next out",
